{
  "name": "nest-status-monitor",
  "version": "0.1.4",
  "description": "Realtime Monitoring for Express-based Node applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GenFirst/nest-status-monitor.git"
  },
  "author": "ivanvs <robince885@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "status",
    "monitoring",
    "node"
  ],
  "devDependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "@nestjs/testing": "^8.3.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/on-headers": "^1.0.0",
    "@types/pidusage": "^2.0.2",
    "coveralls": "^3.1.1",
    "jest": "^27.5.1",
    "nestjs-config": "^1.4.10",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.5",
    "@nestjs/websockets": "^8.3.1",
    "debug": "^4.3.3",
    "handlebars": "^4.7.7",
    "on-headers": "^1.0.2",
    "pidusage": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "request-promise-native": "^1.0.9",
    "rxjs": "^7.5.4",
    "socket.io": "^4.4.1"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "build": "rimraf dist && tsc",
    "format": "prettier src/**/*.ts --ignore-path ./.prettierignore --write && git status"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/GenFirst/nest-status-monitor/issues"
  },
  "homepage": "https://github.com/GenFirst/nest-status-monitor#readme",
  "directories": {
    "example": "examples"
  }
}
